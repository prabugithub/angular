{
  "name": "angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "node --max_old_space_size=20000 ./node_modules/@angular/cli/bin/ng",
    "start": "node --max_old_space_size=20000 ./node_modules/@angular/cli/bin/ng serve --port 4200",
    "build": "ng build",
    "build:aws": "ng build --env=aws",
    "test": "node --max_old_space_size=12000 ./node_modules/@angular/cli/bin/ng test  --sourcemaps=false --cc",
    "combineCoverage": "node combineCoverage.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prod": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build --prod",
    "lint-fix": "tslint --fix src/**/*.ts -t verbose"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.0",
    "@angular-devkit/build-angular": "~15.0.4",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  }
}
